{% load myutils %}
<table rules="rows,cols" border="1" cellpadding="1" cellspacing="1" id="display_search_results">
{% for res in cred_search_results %}
<tr id="row_{{ forloop.counter0 }}" bgcolor="{% cycle '#FFFFFF'  '#F8F8F8' %}" onMouseOver="this.bgColor='#E0FFE0';" onMouseOut="this.bgColor='{% cycle '#FFFFFF'  '#F8F8F8' %}';"><td>
{% if res.url.url|contains:'http' %}
<a id="search_result_url_{{ forloop.counter0 }}" onclick="return mark_row_clicked({{forloop.counter0}}, '{{res.cred.cred_password}}');" href="{{res.url.url}}" target="_blank">{{res.url.url}}</a>
{% else %}
<span id="search_result_url_{{ forloop.counter0 }}">{{res.url.url}}</span>
{% endif %}
</td>
<td id="search_result_username_{{ forloop.counter0 }}" onClick="return mycopyTextToClipboard('{{res.cred.cred_username}}', '-1');">{{res.cred.cred_username}}</td>
<td title="{{res.datelog}}" onClick="return mycopyTextToClipboard('{{res.cred.cred_password}}', '{{ forloop.counter0 }}', {{ res.id }}, '{{ res.qrlink|urlencode }}');">Click to copy</td>
<td><textarea rows=1 cols=30 id="search_result_cred_note_{{ forloop.counter0 }}">{{res.note}}</textarea></td>
<td><a href="{{res.qrlink}}">qrlink</a></td>
<td><input type="button" value="rm" onClick="return cred_delete('{{res.id}}', 'row_{{ forloop.counter0 }}');"/></td>
</tr>
{% endfor %}
</table>
